stack_error stack_ok(int *stk) {

    size_t result = 0, rate = 1;

    C(RED);

    if (stk == NULL) {
        result += rate;
        printf("stk == NULL");
    }

    rate *= 2;
    if (stk->copacity == NULL) {
        result += rate;
        printf("stk->copacity == NULL");
    }

    rate *= 2;
    if (stk->copacity < 0) {
        result += rate;
        printf("stk->copacity < 0");
    }

    rate *= 2;
    if (stk->a == NULL) {
        result += rate;
        printf("stk->a == NULL");
    }

    rate *= 2;
    if (stk->a < 0) {
        result += rate;
        printf("stk->a < 0");
    }

    rate *= 2;
    if (stk->data == NULL) {
        result += rate;
        printf("stk->data == NULL");
    }

    C(WHITE);

    return result;
}

